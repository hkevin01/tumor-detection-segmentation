{
  "model": {
    "name": "QuickExperiment",
    "architecture": "unetr",
    "input_size": [64, 64, 64],
    "in_channels": 4,
    "out_channels": 4,
    "feature_size": 16,
    "hidden_size": 768,
    "mlp_dim": 3072,
    "num_heads": 12,
    "pos_embed": "perceptron",
    "dropout_rate": 0.1
  },
  "training": {
    "batch_size": 2,
    "learning_rate": 0.0003,
    "weight_decay": 0.01,
    "max_epochs": 20,
    "validation_interval": 1,
    "sw_batch_size": 4,
    "overlap": 0.25,
    "cache_rate": 0.9,
    "num_workers": 4,
    "gradient_clipping": true,
    "max_grad_norm": 1.0,
    "save_frequency": 5
  },
  "optimizer": {
    "name": "AdamW",
    "lr": 0.0003,
    "betas": [0.9, 0.999],
    "eps": 1e-8,
    "weight_decay": 0.01
  },
  "scheduler": {
    "name": "ReduceLROnPlateau",
    "mode": "max",
    "factor": 0.5,
    "patience": 3,
    "threshold": 0.001,
    "min_lr": 1e-6,
    "verbose": true
  },
  "loss": {
    "name": "DiceCELoss",
    "to_onehot_y": true,
    "softmax": true
  },
  "mlflow": {
    "enabled": true,
    "experiment_name": "Quick_Experiments",
    "run_name": "fast_adamw_test"
  }
}
